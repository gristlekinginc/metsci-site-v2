{
  "git": {
    "deploymentEnabled": {
      "gh-pages": false,
      "dependabot/npm_and_yarn/docusaurus/plugin-content-blog-3.7.0": false,
      "dependabot/npm_and_yarn/docusaurus/plugin-ideal-image-3.7.0": false,
      "dependabot/npm_and_yarn/docusaurus/preset-classic-3.7.0": false
    }
  },
  "github": {
    "silent": true
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/scripts/(.*).sh",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-sh"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/helium-lns",
      "destination": "/?ref=helium-lns",
      "permanent": false
    }
  ]
} 