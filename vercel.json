{
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "github": {
    "silent": true
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/scripts/(.*).sh",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-sh"
        }
      ]
    }
  ]
} 